{
  "configurations": [
    {
      "name": "x86-Release",
      "generator": "Ninja",
      "configurationType": "Release",
      "inheritEnvironments": [
        "msvc_x86"
      ],
      "buildRoot": "${env.github.workspace}\\build",
      "installRoot": "${env.github.workspace}\\install",
      "cmakeCommandArgs": "-DCMAKE_BUILD_TYPE=Release, -DHAVE_CXX14=ON, -DWIZARD=ON, -DMSVC_HIDE_CONSOLE_WINDOW=ON, -DCMAKE_TOOLCHAIN_FILE=32bit.toolchain",
      "buildCommandArgs": "-v",
      "ctestCommandArgs": ""
    }
  ]
}
